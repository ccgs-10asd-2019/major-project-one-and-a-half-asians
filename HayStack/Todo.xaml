<Page x:Class="HayStack.Todo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HayStack"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Todo">
    <StackPanel>
        <Grid>
            <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="450" Stroke="Black" VerticalAlignment="Top" Width="800"/>
            <Label Content="ToDo" HorizontalAlignment="Left" Margin="453,239,0,0" VerticalAlignment="Top"/>
            <Button x:Name="TodoReturn" Content="RETURN" HorizontalAlignment="Left" Margin="57,154,0,0" VerticalAlignment="Top" Width="280" Height="212" Click="Todo_Return"/>

        </Grid>
    </StackPanel>
</Page>
