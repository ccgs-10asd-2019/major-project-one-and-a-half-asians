<Page x:Class="HayStack.Todo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HayStack"
      mc:Ignorable="d" 
      d:DesignHeight="450"
      Title="Todo" Width="720">
    <StackPanel>
        <Grid >
            <Image HorizontalAlignment="Left" Height="450" VerticalAlignment="Top" Width="720" Source="MainWindow/ToDoTemplate.jpg"/>
            <Button x:Name="TodoReturn" Click="Todo_Return" Opacity="0" FocusVisualStyle="{x:Null}" Margin="659,336,10,61"/>

        </Grid>
    </StackPanel>
</Page>
